<% include ../partials/header %>

<div class="container">
	<div class="row text-center" style="display:flex; flex-wrap: wrap;">
		<% foundList.exerciseList.forEach(function(exercise){ %>
			<div class="col-lg-3 col-md-4 col-sm-6 col-12">
				<div>
					<figure class="figure">
						<a href="/exercises/<%= exercise._id %>"><img class="mainExImg figure-img img-fluid rounded" src="<%= exercise.image %>"></a>
						<figcaption class="figure-caption">
							<%= exercise.name %>1
						</figcaption>
					</figure>
				</div>
			</div>
		<% }); %>
	</div>
</div>

<% include ../partials/footer %>
